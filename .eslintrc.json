{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-extra-semi": "off",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "indent": "off",
        "linebreak-style": "off"
    }
}
